<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="code_for_good.linkage.LauncherActivity">

    <ImageButton
        android:id="@+id/changeLanguage"
        android:layout_width="80dp"
        android:layout_height="45dp"
        android:onClick="changeLangActivity"
        app:srcCompat="@mipmap/gb"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="31dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="29dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="31dp" />

    <Button
        android:id="@+id/toProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        android:onClick="toProfileAct"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="111dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="435dp"
        android:layout_marginRight="327dp" />

</android.support.constraint.ConstraintLayout>
